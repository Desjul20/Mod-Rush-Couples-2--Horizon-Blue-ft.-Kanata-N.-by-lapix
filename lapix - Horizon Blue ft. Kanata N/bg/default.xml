	<Layer
        Type="Sprite"
        Texture="bg.png"
        Frag="horizon.frag"
		OnCommand="%function(self)
		star=self
            self:x(SCREEN_CENTER_X)
            self:y(SCREEN_CENTER_Y)
			self:zoomto(640,480)
	    self:diffusealpha(1)
        end"
	/>
	
